{
  "functions": {
    "source": "functions",
    "ignore": [
      "serviceAccountKey.json",
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-errors.log"
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true,
      "port": 4000
    }
  }
}